{
  "name": "@gouvminint/vue-dsfr",
  "type": "module",
  "version": "3.7.5",
  "packageManager": "pnpm@10.2.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dnum-mi/vue-dsfr.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/vue-dsfr.js"
      }
    },
    "./dist/vue-dsfr.css": {
      "import": "./dist/vue-dsfr.css"
    },
    "./meta": {
      "types": "./meta-dts/index.d.ts",
      "import": "./meta/index.js"
    },
    "./styles": {
      "import": "./dist/vue-dsfr.css"
    }
  },
  "main": "./dist/vue-dsfr.umd.cjs",
  "module": "./dist/vue-dsfr.js",
  "types": "./types",
  "bin": {
    "vue-dsfr-icons": "./meta/custom-icon-collections-creator-bin.js"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "lib",
    "meta",
    "meta-dts",
    "types"
  ],
  "scripts": {
    "check-exports": "node ./ci/check-exports.mjs",
    "check-exports-ci": "node ./ci/check-exports.mjs --ci",
    "clean-dist": "rimraf dist lib",
    "clean": "run-s clean-dist",
    "prebuild": "run-s clean-dist",
    "build": "run-s build:vite type-gen build:meta",
    "build:vite": "vite build",
    "build:meta": "tsc -p tsconfig.meta.json",
    "vitest": "vitest",
    "test": "vitest run",
    "test:watch": "vitest",
    "test-storybook": "cross-env test-storybook --url ${TEST_STORYBOOK_URL:-http://localhost:6006} --browsers chromium",
    "type-gen": "vue-tsc -p tsconfig.app.json --declaration --emitDeclarationOnly",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "dev": "storybook dev -p 6006",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docs:dev": "cross-env VITEPRESS=true vitepress dev",
    "docs:build": "cross-env VITEPRESS=true vitepress build",
    "docs:preview": "cross-env VITEPRESS=true vitepress preview"
  },
  "peerDependencies": {
    "@iconify/vue": "^4.2.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "dependencies": {
    "@gouvfr/dsfr": "~1.13.0",
    "focus-trap": "^7.6.4",
    "focus-trap-vue": "^4.0.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.1",
    "@chromatic-com/storybook": "^4.1.2",
    "@iconify/vue": "^4.3.0",
    "@playwright/test": "1.49.1",
    "@storybook/addon-a11y": "^10.0.2",
    "@storybook/addon-docs": "^10.0.2",
    "@storybook/addon-links": "^10.0.2",
    "@storybook/addon-themes": "^10.0.2",
    "@storybook/test-runner": "^0.24.1",
    "@storybook/vue3-vite": "^10.0.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^12.5.0",
    "@vueuse/shared": "^12.5.0",
    "@whyframe/core": "^0.1.12",
    "@whyframe/vue": "^0.1.7",
    "browserslist": "^4.24.4",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.20.0",
    "eslint-plugin-storybook": "10.0.2",
    "eslint-plugin-vue": "^9.32.0",
    "globby": "^14.1.0",
    "http-server": "^14.1.1",
    "inquirer": "^12.4.1",
    "jsdom": "^26.0.0",
    "lightningcss": "^1.29.1",
    "npm-run-all": "^4.1.5",
    "p-debounce": "^4.0.0",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^6.0.1",
    "start-server-and-test": "^2.0.10",
    "storybook": "^10.0.2",
    "typescript": "~5.7.3",
    "unplugin-lightningcss": "^0.3.0",
    "vite": "^6.1.0",
    "vite-plugin-full-reload": "^1.2.0",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.2.0"
  }
}
