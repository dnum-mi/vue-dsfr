{
  "name": "@laruiss/vue-dsfr",
  "version": "1.0.0-beta.5",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-dsfr.umd.js",
  "module": "./dist/vue-dsfr.esm.js",
  "exports": {
    ".": {
      "import": "./dist/vue-dsfr.esm.js",
      "require": "./dist/vue-dsfr.umd.js"
    },
    "./css": {
      "import": "./dist/vue-dsfr.css",
      "require": "./dist/vue-dsfr.css"
    },
    "./fonts": {
      "import": "./dist/vue-dsfr-fonts.css",
      "require": "./dist/vue-dsfr-fonts.css"
    }
  },
  "publishConfig": {
    "@laruiss:registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "dev": "npm run storybook",
    "prebuild": "rimraf dist",
    "build": "run-p build:css build:js",
    "build:css": "node build-css.mjs",
    "build:js": "cross-env NODE_ENV=production rollup --config rollup.config.js",
    "serve": "vite preview",
    "test": "jest",
    "format": "npm run lint -- --fix",
    "lint": "eslint ./src/**/*.{vue,js,jsx,ts,tsx}",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@storybook/addon-docs": "^6.3.6",
    "core-js": "^3.8.3",
    "jest-transform-stub": "^2.0.0",
    "oh-vue-icons": "^0.4.3",
    "vue": "^3.0.5",
    "vuepress": "^2.0.0-beta.20"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/plugin-syntax-jsx": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@cypress/vite-dev-server": "^2.0.1",
    "@rollup/plugin-alias": "^3.1.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-url": "^6.0.0",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/client-api": "^6.3.4",
    "@storybook/core-common": "^6.3.4",
    "@storybook/vue3": "^6.3.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/vue": "^6.4.2",
    "@vitejs/plugin-vue": "^1.2.4",
    "@vitejs/plugin-vue-jsx": "^1.1.6",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-standard": "^6.0.0",
    "@vue/test-utils": "^2.0.0-rc.10",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "cypress": "^7.6.0",
    "eslint": "^7.29.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.12.1",
    "globby": "^11.0.4",
    "jest": "^26.6.3",
    "jest-svg-transformer": "^1.0.0",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "postcss-csso": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-font-base64": "^1.0.5",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.5",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.3",
    "rimraf": "^3.0.2",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.5.0",
    "rollup-plugin-vue": "^6.0.0",
    "semantic-release": "^17.4.4",
    "storybook-builder-vite": "^0.0.12",
    "vite": "^2.3.8",
    "vue-jest": "^5.0.0-0",
    "vue-loader": "^16.2.0",
    "vue-router": "^4.0.10",
    "vue-router-mock": "^0.0.3",
    "vuepress-vite": "^2.0.0-beta.20",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/laruiss/vue-dsfr.git"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true,
        "branch": "develop"
      }
    ]
  }
}
