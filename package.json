{
  "name": "@gouvminint/vue-dsfr",
  "type": "module",
  "version": "3.7.5",
  "packageManager": "pnpm@10.24.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dnum-mi/vue-dsfr.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "provenance": true,
    "tag": "latest"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/vue-dsfr.js"
      }
    },
    "./dist/vue-dsfr.css": {
      "import": "./dist/vue-dsfr.css"
    },
    "./meta": {
      "types": "./meta-dts/index.d.ts",
      "import": "./meta/index.js"
    },
    "./styles": {
      "import": "./dist/vue-dsfr.css"
    }
  },
  "main": "./dist/vue-dsfr.umd.cjs",
  "module": "./dist/vue-dsfr.js",
  "types": "./types",
  "bin": {
    "vue-dsfr-icons": "./meta/custom-icon-collections-creator-bin.js"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "lib",
    "meta",
    "meta-dts",
    "types"
  ],
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "check-exports": "node ./ci/check-exports.mjs",
    "check-exports-ci": "node ./ci/check-exports.mjs --ci",
    "clean-dist": "rimraf dist lib",
    "prebuild": "run-s clean-dist",
    "build": "run-s build:vite type-gen build:meta",
    "build:vite": "vite build",
    "build:meta": "tsc -p tsconfig.meta.json",
    "demo": "vite",
    "build:demo": "vite -c vite.config.demo.js build",
    "dev": "storybook dev -p 6006",
    "vitest": "vitest",
    "coverage": "vitest run --coverage",
    "test-storybook": "cross-env test-storybook --url ${TEST_STORYBOOK_URL:-http://localhost:6006} --browsers chromium",
    "test": "run-s test:unit",
    "test:watch": "vitest",
    "test:unit": "vitest run",
    "test:storybook": "vitest --project=storybook",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "lint:staged": "eslint ",
    "prepublishOnly": "run-s build",
    "semantic-release": "semantic-release",
    "type-gen": "vue-tsc -p tsconfig.app.json --declaration --emitDeclarationOnly",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docs:dev": "cross-env VITEPRESS=true vitepress dev",
    "docs:build": "cross-env VITEPRESS=true vitepress build",
    "docs:build-only": "cross-env VITEPRESS=true vitepress build",
    "docs:preview": "cross-env VITEPRESS=true vitepress preview",
    "commitlint": "commitlint --edit"
  },
  "peerDependencies": {
    "@iconify/vue": "^4.2.0",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4"
  },
  "dependencies": {
    "@gouvfr/dsfr": "~1.14.3",
    "focus-trap": "^7.8.0",
    "focus-trap-vue": "^4.1.0",
    "vue": "^3.5.27",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@chromatic-com/storybook": "^4.1.3",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@iconify/vue": "^4.3.0",
    "@playwright/test": "1.57.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^13.1.4",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-links": "^10.2.8",
    "@storybook/addon-themes": "^10.2.8",
    "@storybook/addon-vitest": "^10.2.8",
    "@storybook/test-runner": "^0.24.2",
    "@storybook/vue3-vite": "^10.2.8",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.13",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "@vueuse/core": "^12.8.2",
    "@vueuse/shared": "^12.8.2",
    "@whyframe/core": "^0.1.14",
    "@whyframe/vue": "^0.1.9",
    "browserslist": "^4.28.1",
    "chalk": "^5.6.2",
    "commander": "^13.1.0",
    "commitlint": "^19.8.1",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-storybook": "10.0.2",
    "eslint-plugin-vue": "^9.33.0",
    "globby": "^16.1.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "inquirer": "^12.11.1",
    "jsdom": "^27.4.0",
    "lightningcss": "^1.31.1",
    "lint-staged": "^15.5.2",
    "npm-run-all": "^4.1.5",
    "p-debounce": "^5.1.0",
    "playwright": "^1.58.2",
    "pnpm": "^10.24.0",
    "publint": "^0.3.17",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^6.1.3",
    "semantic-release": "^25.0.3",
    "start-server-and-test": "^2.1.3",
    "storybook": "^10.2.8",
    "typescript": "~5.9.3",
    "unplugin-lightningcss": "^0.3.3",
    "vite": "npm:rolldown-vite@7.3.0",
    "vite-node": "^5.3.0",
    "vite-plugin-full-reload": "^1.2.0",
    "vitepress": "^1.6.4",
    "vitest": "^4.0.18",
    "vue-demi": "^0.14.10",
    "vue-tsc": "^3.2.4"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "run-s check-exports-ci lint --"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true,
        "channel": "beta"
      },
      {
        "name": "next",
        "prerelease": true,
        "channel": "next"
      }
    ],
    "assets": [
      "dist/*.js",
      "dist/*.css"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": false
            },
            {
              "type": "style",
              "release": false
            }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "feat",
                "section": "‚ú® Nouvelles fonctionnalit√©s"
              },
              {
                "type": "fix",
                "section": "üêõ Corrections de bugs"
              },
              {
                "type": "docs",
                "section": "üìö Documentation"
              },
              {
                "type": "perf",
                "section": "‚ö° Am√©liorations de performance"
              },
              {
                "type": "ci",
                "section": "üë∑ CI/CD"
              },
              {
                "type": "test",
                "section": "‚úÖ Tests"
              },
              {
                "type": "refactor",
                "hidden": true
              },
              {
                "type": "style",
                "hidden": true
              }
            ]
          }
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "volta": {
    "node": "24.10.0"
  }
}
