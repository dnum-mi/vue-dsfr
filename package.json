{
  "name": "@gouvminint/vue-dsfr",
  "type": "module",
  "version": "3.6.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dnum-mi/vue-dsfr.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/vue-dsfr.js"
      }
    },
    "./dist/vue-dsfr.css": {
      "import": "./dist/vue-dsfr.css"
    },
    "./styles": {
      "import": "./dist/vue-dsfr.css"
    }
  },
  "main": "./dist/vue-dsfr.umd.cjs",
  "module": "./dist/vue-dsfr.js",
  "types": "./types",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "lib",
    "types"
  ],
  "engines": {
    "node": ">=20.x.x",
    "npm": ">=10.x.x"
  },
  "publishConfig": {
    "@gouvminint:registry": "https://registry.npmjs.com",
    "access": "public"
  },
  "scripts": {
    "prepare": "husky install",
    "check-exports": "node ./ci/check-exports.mjs",
    "check-exports-ci": "node ./ci/check-exports.mjs --ci",
    "clean-dist": "rimraf dist lib",
    "prebuild": "run-s clean-dist",
    "build": "run-s build:vite type-gen",
    "build:vite": "vite build",
    "demo": "vite",
    "build:demo": "vite -c vite.config.demo.js build",
    "dev": "storybook dev -p 6006",
    "vitest": "vitest",
    "coverage": "vitest run --coverage",
    "test-storybook": "test-storybook --url $TEST_STORYBOOK_URL",
    "test": "run-s test:unit test:ct-ci",
    "test:watch": "vitest",
    "test:unit": "vitest run",
    "test:ct": "cypress open --component",
    "test:ct-ci": "cypress run --component",
    "format": "run-s lint -- --fix",
    "lint": "eslint .",
    "lint:staged": "eslint ",
    "prepublishOnly": "run-s build",
    "semantic-release": "semantic-release",
    "type-gen": "vue-tsc -p tsconfig.app.json --declaration --emitDeclarationOnly",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "patch-oh-vue-icons": "patch-package",
    "docs:dev": "cross-env VITEPRESS=true vitepress dev",
    "docs:build": "run-s patch-oh-vue-icons docs:build-only",
    "docs:build-only": "cross-env VITEPRESS=true vitepress build",
    "docs:preview": "cross-env VITEPRESS=true vitepress preview"
  },
  "peerDependencies": {
    "oh-vue-icons": "1.0.0-rc3",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "dependencies": {
    "@gouvfr/dsfr": "~1.12.1",
    "focus-trap": "^7.5.4",
    "focus-trap-vue": "^4.0.3",
    "oh-vue-icons": "1.0.0-rc3",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.2",
    "@chromatic-com/storybook": "^1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@cypress/vite-dev-server": "^5.1.1",
    "@cypress/vue": "^6.0.1",
    "@storybook/addon-a11y": "^8.1.11",
    "@storybook/addon-docs": "^8.1.11",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-mdx-gfm": "^8.1.11",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/addon-themes": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/test": "^8.1.11",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/theming": "^8.1.11",
    "@storybook/vue3": "^8.1.11",
    "@storybook/vue3-vite": "^8.1.11",
    "@testing-library/dom": "^10.3.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.1.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.9",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "@vueuse/core": "^10.11.0",
    "@vueuse/shared": "^10.11.0",
    "@whyframe/core": "^0.1.11",
    "@whyframe/vue": "^0.1.7",
    "browserslist": "^4.23.1",
    "cross-env": "^7.0.3",
    "cypress": "^13.13.0",
    "eslint": "^9.6.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lightningcss": "^1.25.1",
    "lint-staged": "^15.2.7",
    "npm-run-all": "^4.1.5",
    "p-debounce": "^4.0.0",
    "patch-package": "^8.0.0",
    "playwright": "^1.45.1",
    "postcss": "^8.4.39",
    "postcss-csso": "^6.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.5.15",
    "postcss-url": "^10.1.3",
    "publint": "^0.2.8",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^5.0.7",
    "semantic-release": "^23.1.1",
    "start-server-and-test": "^2.0.4",
    "storybook": "^8.1.11",
    "typescript": "~5.3.3",
    "vite": "^5.3.3",
    "vite-node": "^1.6.0",
    "vite-plugin-full-reload": "^1.1.0",
    "vitepress": "^1.2.3",
    "vitest": "^1.6.0",
    "vue-demi": "^0.14.8",
    "vue-tsc": "^2.0.24"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "run-s check-exports-ci lint --"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true,
        "channel": "beta"
      },
      {
        "name": "next",
        "prerelease": true,
        "channel": "next"
      }
    ],
    "assets": [
      "dist/*.js",
      "dist/*.css"
    ]
  },
  "volta": {
    "node": "20.11.0"
  }
}
