{
  "name": "vue-dsfr-monorepo",
  "type": "module",
  "version": "3.7.5",
  "packageManager": "pnpm@10.2.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dnum-mi/vue-dsfr.git"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "test:unit": "turbo run test:unit",
    "dev": "turbo run dev --parallel",
    "demo": "turbo run dev --filter=demo-vue-dsfr",
    "storybook": "turbo run storybook --filter=@gouvminint/vue-dsfr",
    "build-storybook": "turbo run build-storybook --filter=@gouvminint/vue-dsfr",
    "test-storybook": "turbo run test-storybook --filter=@gouvminint/vue-dsfr",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "clean": "turbo run clean",
    "docs:dev": "cross-env VITEPRESS=true vitepress dev",
    "docs:build": "cross-env VITEPRESS=true vitepress build",
    "docs:preview": "cross-env VITEPRESS=true vitepress preview",
    "commitlint": "commitlint --edit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.1",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "commitlint": "^19.7.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.20.0",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "pnpm": "^10.2.1",
    "turbo": "^2.3.4",
    "vitepress": "^1.6.3"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "turbo run lint --"
  },
  "volta": {
    "node": "22.13.1"
  }
}
